 -->
      <class name="AutomationTest.GoDirectVoiceAndroid.Keypad.KeypadProfileSIPSelect"/>
       <class name="AutomationTest.GoDirectVoiceAndroid.Keypad.KeypadProfileIAXSelect"/>
      
