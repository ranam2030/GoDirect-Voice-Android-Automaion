<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test thread-count="5" name="Test">
    <classes>
	<!--TC#544	Press on Re-Sync Account Details button and verify Re-sync Successful pop up when account is Re-synced successfully -->
